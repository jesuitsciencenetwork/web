digraph {
graph [overlap = false, splines = true];
{% for n in nodes %}
{{ n.id }} [shape=box,{% if n.group == 'n' %}style=dashed,{% endif %}label="{{ n.label }}"];
{% endfor %}

{% for e in edges %}
{% if e.arrows == 'to' %}{{ e.from }} -> {{ e.to }}{% else %}{{ e.to }} -> {{ e.from }}{% endif %} [];
{% endfor %}
}

